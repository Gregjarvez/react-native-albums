{  "plugins": [    "react",    "react-native"  ],  "parser": "babel-eslint",  "parserOptions": {    "ecmaVersion": 2015,    "sourceType": "module"  },  "env": {    "browser": true,    "node": true,    "jest": true,    "es6": true  },  "extends": ["airbnb", "plugin:react-native/all"],  "rules": {    "react/prefer-stateless-function": 0,    "react/jsx-filename-extension": 0,    "comma-dangle": "off",    "no-use-before-define": 1,    "react-native/no-color-literals": 1,    "react/require-default-props": 0  }}